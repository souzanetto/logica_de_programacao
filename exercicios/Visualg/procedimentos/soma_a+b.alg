algoritmo "semnome"
var
   X, Y: inteiro
   Procedimento Soma(A, B: inteiro)
   Inicio
         Escreval("O valor de A é ", A)
         Escreval("O valor de B é ", B)
         Escreval("O valor da soma de A + B é igual a: ", A+B)
   FimProcedimento
inicio
      X <- 5
      Y <- 3
      Soma(X, Y)
fimalgoritmo
