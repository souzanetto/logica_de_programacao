algoritmo "semnome"
var
    cont, num, num2, fibo: inteiro
inicio
      num <- 0
      num2 <- 1
      fibo <- 0
      Escreva(num , num2)
      
      Para cont <- 1 ate 13 faca
      fibo <- num + num2
      Escreva(fibo)
      num <- num2
      num2 <- fibo
      FimPara
      
      
fimalgoritmo
