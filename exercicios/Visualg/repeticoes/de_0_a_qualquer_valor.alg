algoritmo "semnome"
var
    contador, valor, salto: inteiro
inicio
      escreva("Qual o valor da contagem?")
      leia(valor)
      escreva("Qual o valor do salto?")
      leia(salto)
      contador <- 0
      Enquanto (contador <= valor) faca
      escreval(contador)
      contador <- contador + salto
      FimEnquanto
      escreva("A contagem chegou ao fim!")
fimalgoritmo
