algoritmo "Par ou Ímpar"
var
   num: inteiro
inicio
       Escreva("Digite um número: ")
       leia(num)
       
       Se(num%2 = 0)entao
          Escreval("")
          Escreva("O número ", num ," é PAR!")
          Escreval("")
        Senao
           Escreval("")
           Escreva("O número ", num ," é ÍMPAR!")
           Escreval("")
       FimSe
fimalgoritmo

