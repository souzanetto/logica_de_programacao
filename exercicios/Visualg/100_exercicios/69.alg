algoritmo "PA"
var
   cont, num, PA, razao: inteiro
inicio

      Escreva("Digite o primeiro valor da PA: ")
      leia(num)
      Escreva("E a razão? ")
      leia(razao)

         Escreva(num)
      Para cont <- 1 ate 9 faca
         PA <- num + razao
         Escreva(PA)
         num <- PA
      FimPara
      
fimalgoritmo
