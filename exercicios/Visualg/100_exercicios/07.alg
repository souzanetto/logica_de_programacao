algoritmo "Dobro e Terça Parte"
var
      num, dobro, terco: real
inicio
      Escreva("Digite um número: ")
      leia(num)
      
      dobro <- num*2
      terco <- num/3
      
      Escreval("O dobro de", num, " é", dobro)
      Escreval("A terça parte de", num, " é", terco)
      
fimalgoritmo
